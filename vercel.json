{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/fonts/(.*)", "destination": "/fonts/$1" },
    { "source": "/sw.js", "destination": "/sw.js" },
    { "source": "/site.webmanifest", "destination": "/site.webmanifest" },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/ANAS-FRAGRANCES.ico", "destination": "/ANAS-FRAGRANCES.ico" },
    { "source": "/ANAS-FRAGRANCES.png", "destination": "/ANAS-FRAGRANCES.png" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}